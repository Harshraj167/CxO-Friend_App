{
  "version": 2,
  "builds": [
    {
      "src": "app/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "app/frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api.cxofriend.com/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app/frontend/$1"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://api.cxofriend.com",
    "VITE_ENV": "production",
    "VITE_VERSION": "6.3.0"
  }
}
